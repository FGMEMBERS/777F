<!--  controls -->

    <animation>
        <type>translate</type>
        <object-name>RHpedal</object-name>
        <property>controls/flight/rudder</property>
        <factor>0.05</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>LHpedal</object-name>
        <property>controls/flight/rudder</property>
        <factor>-0.05</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Yokeshaft</object-name>
        <object-name>Yoke</object-name>
        <object-name>Yoke.001</object-name>
        <property>controls/flight/elevator</property>
        <factor>15</factor>
        <center>
            <x-m>-1.596</x-m>
            <y-m>0</y-m>
            <z-m>-0.257</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Yoke</object-name>
        <property>controls/flight/aileron</property>
        <factor>30</factor>
        <center>
            <x-m>-1.574</x-m>
            <y-m>0.559</y-m>
            <z-m>0.336</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Yoke.001</object-name>
        <property>controls/flight/aileron</property>
        <factor>30</factor>
        <center>
            <x-m>-1.574</x-m>
            <y-m>-0.526</y-m>
            <z-m>0.336</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>Cseat.armrest</object-name>
        <property>sim/model/armrest</property>
        <factor>93</factor>
        <center>
            <x-m>-0.928</x-m>
            <y-m>0</y-m>
            <z-m>0.127</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Pseat.armrest</object-name>
        <property>sim/model/armrest</property>
        <factor>93</factor>
        <center>
            <x-m>-0.928</x-m>
            <y-m>0</y-m>
            <z-m>0.127</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>Pseat.armrest</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-toggle</command>
                <property>sim/model/armrest</property>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>Cseat.armrest</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-toggle</command>
                <property>sim/model/armrest</property>
            </binding>
        </action>
    </animation>
    
    <animation>
        <type>pick</type>
        <object-name>Mstr.Warning</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>nasal</command>
                <script>b777.Weu.mute_warnings();</script>
            </binding>
        </action>
    </animation>

    <animation>
        <type>select</type>
        <object-name>Mstr.Warning.001</object-name>
        <condition>
            <or>
                <property>instrumentation/mk-viii/outputs/discretes/gpws-warning</property>
                <property>instrumentation/mk-viii/outputs/discretes/gpws-alert</property>
                <property>instrumentation/weu/light/master-warning</property>
            </or>
        </condition>
    </animation>
    
    <animation>
        <type>select</type>
        <object-name>Mstr.Caution.001</object-name>
        <condition>
            <property>instrumentation/weu/light/master-caution</property>
        </condition>
    </animation>

</PropertyList>